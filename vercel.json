{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["api/**"] }
      },
      {
        "src": "fronted/**",
        "use": "@vercel/static",
        "config": { "includeFiles": ["frontend/**"] }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/",
        "dest": "frontend/index.html"
      }
    ]
}
